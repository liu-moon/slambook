{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ROS: Launch my file",
      "request": "launch",
      "target": "${workspaceFolder}/install/",
      "launch": [
        "rviz",
        "gz",
        "gzserver",
        "gzclient"
      ],
      "type": "ros"
    },
    {
      "name": "C++ Debugger",
      "request": "launch",
      "type": "cppdbg",
      "cwd": "${workspaceFolder}/",
      // "args": ["/home/liuiu/桌面/slambook/src/ch5/src/ubuntu.png"],
      "program": "${workspaceFolder}/install/ch5/lib/ch5/jointMap",
      "miDebuggerPath": "/usr/bin/gdb"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/liuiu/桌面/slambook/doc",
      "program": "/home/liuiu/桌面/slambook/doc/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}